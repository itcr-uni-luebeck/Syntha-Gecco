        <archetype_id>
          <value>openEHR-EHR-OBSERVATION.medication_statement.v0</value>
        </archetype_id>
        <term_definitions code="at0000">
          <items id="description">Eine Snapshot-Ansicht über die Anwendung eines bestimmten Medikaments.</items>
          <items id="text">Medikationseintrag</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">@ internal @</items>
          <items id="text">History</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="description">Standardwert, ein undefinierter/s Zeitpunkt oder Intervallereignis, das explizit im Template oder zur Laufzeit der Anwendung definiert werden kann.</items>
          <items id="text">Beliebiges Ereignis</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">@ internal @</items>
          <items id="text">Tree</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">@ internal @</items>
          <items id="text">Item tree</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="comment">Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen.</items>
          <items id="description">Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.</items>
          <items id="text">Erweiterung</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="comment">Es wird dringend empfohlen, das „Arzneimittel“ mit einer Terminologie zu codieren, die nach Möglichkeit Entscheidungsunterstützung auslösen kann. Das Ausmaß der Codierung kann vom einfachen generischen oder Handelsnamen des Medikamentes bis hin zu strukturierten Angaben über die tatsächlich zu verwendende Medikamentenpackung variieren. Die Freitexteingabe sollte nur verwendet werden, wenn keine geeignete Terminologie verfügbar ist.</items>
          <items id="description">Name des Medikaments, des Impfstoffs oder eines anderen therapeutischen / verschreibungsfähigen Mittels.</items>
          <items id="text">Arzneimittel-Name</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="comment">Verwenden Sie diesen SLOT, in dem die detaillierte Beschreibung des angeordneten Arzneimittels explizit angegeben werden muss. Zum Beispiel: Form, Wirkstärke, Verdünnungsmittel oder Mischung von Wirkstoffen.</items>
          <items id="description">Strukturierte Angaben zur Gesamtzubereitung einschließlich Wirkstärke, Verabreichungsform und Inhaltsstoffe.</items>
          <items id="text">Herstellungsdetails</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="comment">Zum Beispiel: Das Medikament wird immer noch aktiv eingenommen oder eine Antibiotikatherapie wurde abgeschlossen.</items>
          <items id="description">Der Status der Anwendung des Medikaments.</items>
          <items id="text">Status</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="description">Das Medikament soll in Zukunft zu einem nicht festgelegten Zeitpunkt angewendet werden.</items>
          <items id="text">Vorgesehen</items>
        </term_definitions>
        <term_definitions code="at0010">
          <items id="description">Das Medikament soll zu einem späteren Zeitpunkt eingenommen werden.</items>
          <items id="text">Geplant</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Das Medikament wird derzeit angewendet.</items>
          <items id="text">Aktiv</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="description">Das Medikament wird nicht mehr angewendet.</items>
          <items id="text">Abgeschlossen</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="description">Die Anwendung des Medikaments ist geplant, der Beginn wurde jedoch vorübergehend verzögert.</items>
          <items id="text">Verschoben</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Das Medikament sollte angewendet werden, wurde jedoch vor Beginn abgesagt.</items>
          <items id="text">Widerrufen</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Die Medikamentenanwendung wurde begonnen, aber vorübergehend ausgesetzt.</items>
          <items id="text">Unterbrochen</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Die Medikamentenanwendung wurde begonnen und dann eingestellt.</items>
          <items id="text">Abgebrochen</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Der Status der Medikamentenanwendung ist nicht bekannt.</items>
          <items id="text">Unbekannt</items>
        </term_definitions>
        <term_definitions code="at0018">
          <items id="comment">Zum Beispiel: wegen Nebenwirkung abgebrochen; wegen anstehender Operation ausgesetzt.</items>
          <items id="description">Begründung für den Status.</items>
          <items id="text">Statusgrund</items>
        </term_definitions>
        <term_definitions code="at0019">
          <items id="description">Der gültige Zeitpunkt für den Statusbeginn.</items>
          <items id="text">Status Startzeit</items>
        </term_definitions>
        <term_definitions code="at0020">
          <items id="comment">Dieses Datenelement kann entweder mit der Status Startzeit oder der Status Endzeit kombiniert werden, jedoch nicht mit beiden.</items>
          <items id="description">Die gültige Dauer für den Status.</items>
          <items id="text">Status Dauer</items>
        </term_definitions>
        <term_definitions code="at0021">
          <items id="description">Der gültige Zeitpunkt für das Statusende.</items>
          <items id="text">Status Endzeit</items>
        </term_definitions>
        <term_definitions code="at0022">
          <items id="description">Details zu strukturierten Dosis- und Zeitangaben.</items>
          <items id="text">Strukturierte Dosis- und Zeitangaben</items>
        </term_definitions>
        <term_definitions code="at0023">
          <items id="comment">Zum Beispiel: "Angina". Die Codierung der klinischen Indikation mit einer Terminologie wird nach Möglichkeit bevorzugt. Dieses Datenelement kann mehrfach vorkommen.</items>
          <items id="description">Der klinische Grund für die Anwendung des Medikaments.</items>
          <items id="text">Klinische Indikation</items>
        </term_definitions>
        <term_definitions code="at0024">
          <items id="description">Das Datum / die Uhrzeit der ersten Verschreibung des Arzneimittels.</items>
          <items id="text">Zuerst verschrieben</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Das Datum / die Uhrzeit der letzten Verschreibung des Arzneimittels.</items>
          <items id="text">Zuletzt verschrieben</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Das Datum / die Uhrzeit der letzten Einnahme oder Verabreichung des Arzneimittels durch die Person.</items>
          <items id="text">Zuletzt verabreicht</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Das Datum / die Uhrzeit der letzten Überprüfung der Medikamentenanwendung von einem Kliniker oder Apotheker.</items>
          <items id="text">Zuletzt überprüft</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Zeitpunkt der Einstellung des Medikaments.</items>
          <items id="text">Abgesetzt</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="description">Zusätzliche Kommentare über den Medikationseintrag, die nicht in anderen Feldern erfasst wurden.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="comment">Zum Beispiel: "oral", "intravenös" oder "topisch". Die Codierung des Verabreichungswegs mit einer Terminologie wird nach Möglichkeit bevorzugt. Es können mehrere mögliche Verabreichungswege angegeben werden, wenn das Medikament vorgesehen oder geplant ist. Wenn das Medikament verabreicht wurde, ist nur ein Verabreichungsweg relevant.</items>
          <items id="description">Art der Anwendung des Arzneimittels am Körper.</items>
          <items id="text">Verabreichungsweg</items>
        </term_definitions>
